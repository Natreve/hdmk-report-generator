            <h1>Inspection Report</h1>
        </div>

        <div class="cover-image">
            {{image inspection.cover}}
        </div>

        <div class="background"></div>
        <div class="details">
            <article>
                {{#with inspection}}
                <h4><strong>Client</strong></h4>
                <p>{{client.name}}</p>
                {{#with address}}
                <p>{{street}}</p>
                <p>{{city}}, {{state}} {{zipcode}} </p>
                {{/with}}
                {{/with}}
            </article>
            <article>
                {{#with inspector}}
                <h4><strong>Inspector</strong></h4>
                <p>{{fname}} {{lname}}, {{licence_number}}</p>
                <p><a href="tel:{{phone}}">{{phone}}</a></p>
                <p><a href="mailto:{{email}}">{{email}}</a></p>
                {{/with}}
            </article>
            <article>
                <h4><strong>Date</strong></h4>
                <p>{{#date inspection.date.started}}{{/date}}</p>
            </article>
        </div>
    </section>

    <table>
        <thead>
            <tr>
                <td>
                    <div class="header-spacer"></div>
                </td>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>
                    <!-- Table of Content -->
                    <section class="page">
                        <div class="content">
                            <h1>Inspection Report Index</h1>
                            {{#if sectionSummary.length}}
                            <div id="toc">
                                <div><a href="#conditions">1. inspection conditions</a></div>
                                {{#if sections.length}}
                                <div><a href="#limitations">2. report limitations</a></div>
                                {{/if}}
                                <div><a href="#summary">3. inspection summary</a></div>

                                {{#if sections.length}}
                                {{#each sections}}
                                <div><a href="#{{name}}">{{inc @index "4"}}. {{name}}</a> </div>
                                {{/each}}
                                {{/if}}
                            </div>
                            {{else}}
                            <div id="toc">
                                <div><a href="#conditions">1. inspection conditions</a></div>
                                <div><a href="#limitations">2. report limitations</a></div>
                                {{#if sections.length}}
                                {{#each sections}}
                                <div><a href="#{{name}}">{{inc @index "3"}}. {{name}}</a> </div>
                                {{/each}}
                                {{/if}}
                            </div>
                            {{/if}}
                        </div>
                    </section>


                    <!-- Conditions -->
                    <section class="page">
                        <div class="content">
                            <h1 id="conditions">inspection conditions</h1>
                            <!--Client & Site Information-->
                            <article>
                                <h2>client & site information</h2>
                                {{#with inspection}}
                                <table>
                                    <tr>
                                        <td>inspection date:</td>
                                        <td>{{#date date.started}}{{/date}}</td>
                                    </tr>
                                    <tr>
                                        <td>start time:</td>
                                        <td>{{#time date.started}}{{/time}}</td>
                                    </tr>
                                    <tr>
                                        <td>end time:</td>
                                        <td>{{#time date.ended}}{{/time}}</td>
                                    </tr>
                                    <tr>
                                        <td>city & state:</td>
                                        <td>{{address.city}} {{address.state}}</td>
                                    </tr>
                                </table>
                                {{/with}}
                            </article>
                            <!--Payment Information-->
                            <article>
                                <h2>payment information</h2>
                                <table>
                                    <tr>
                                        <td>total fee:</td>
                                        <td>${{inspection.fee}}</td>
                                    </tr>
                                </table>
                            </article>
                            <!--Conditions-->
                            {{#if conditions.length}}
                            {{#each conditions}}
                            <article>
                                <h2>{{this.name}}</h2>
                                <table>
                                    {{#each this.comments}}

                                    <tr>
                                        <td>{{capitalize this.name}}</td>
                                        <td>
                                            <p style="white-space:pre-wrap;">{{this.text}}</p>
                                        </td>
                                    </tr>

                                    {{/each}}
                                </table>
                            </article>
                            {{/each}}
                            {{/if}}
                        </div>
                    </section>

                    <!-- Limitations -->
                    {{#if sections.length}}
                    <section class="page">
                        <div class="content">
                            <h1 id="limitations">report limitations</h1>
                            <p>
                                This inspection report is the intellectual property of HDMK Home Inspection and is
                                intended for the exclusive use of the client for whom it was prepared. It is not to be
                                copied or disseminated to any other party without the expressed written consent of HDMK
