            <p><a href="mailto:{{email}}">{{email}}</a></p>
            {{/with}}
        </div>
    </header>

    <table>
        <thead>
            <tr>
                <td>
                    <div class="header-spacer"></div>
                </td>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>

                    <!-- Report Sections -->
                    {{#if sections.length}}
                    {{#each sections}}
                    <section class="page">
                        <div class="content">
                            <h1 id="{{this.name}}"><a href="#toc">{{this.name}}</a> </h1>

                            {{#each this.items}}
                            <p><strong>{{capitalize this.name}}</strong></p>
                            {{#each this.comments}}
                            <table style="width: 100%;">
                                <tr>
                                    <td style="width: 30%; vertical-align:baseline;">
                                        <p>{{capitalize this.name}}</p>
                                    </td>
                                    <td style="width: 80%;">
                                        <p style="white-space:pre-wrap;">{{this.text}}</p>
                                    </td>
                                </tr>
                            </table>

                            <ul class="images">{{images this.media 165 165}}</ul>
                            <br>
                            {{/each}}
                            {{/each}}
                        </div>

                    </section>
                    {{/each}}
                    {{/if}}
                </td>
            </tr>
        </tbody>
    </table>
</body>

</html>